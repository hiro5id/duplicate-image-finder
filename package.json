{
  "name": "console-log-json",
  "version": "0.0.0",
  "description": "find duplicate images",
  "private": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/src/cli.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.13",
    "npm": ">=6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hiro5id/duplicate-image-finder.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "image",
    "duplicate"
  ],
  "scripts": {
    "prebuild": "echo 'runnign create-ts-index' && cti --withoutbackup create ./src && rimraf **/index.ts.bak",
    "build": "npm run build:tsc",
    "build:tsc": "rimraf ./dist && tsc -sourcemap",
    "build:watch": "npm run build:tsc -- -w",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run prettier:fix",
    "eslint": "eslint --ignore-path .eslintignore . --ext .ts",
    "start": "node --require source-map-support/register ./dist/src/cli.js",
    "clean": "rimraf dist node_modules",
    "test": "mocha \"dist/test/**/*.js\"",
    "audit:lerna-packages": "SCRIPT_DIR=\"$(pwd)\" && echo $SCRIPT_DIR && bash npm-audit.sh && lerna exec --stream --concurrency 1 --no-bail -- bash \"$SCRIPT_DIR/npm-audit.sh\"",
    "prepublishOnly": "npm run clean && npm run install:prod && npm run build && npm run lint && npm run test && npm run prune:prod",
    "preversion": "npm run build && npm run test",
    "prettier": "prettier \"(src|test)/**/*.ts\"",
    "prettier:check": "npm run prettier -- -c",
    "prettier:fix": "npm run prettier -- --write",
    "install:prod": "npm ci",
    "prune:prod": "npm prune --production",
    "refresh-package-locks": "npm run clean && find . -iname package-lock.json -delete && npm install",
    "publish": "np",
    "dev-install-command": "npm link",
    "dev-uninstall-command": "npm unlink"
  },
  "bin": {
    "find-dupes": "./dist/src/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.2",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "chai": "^4.2.0",
    "create-ts-index": "^1.13.6",
    "eslint": "^7.12.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.3.0",
    "mocha": "^8.2.0",
    "np": "^7.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
